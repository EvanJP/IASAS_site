        <div id="sprints" class="col s12">
            <h4>Sprints</h4>
            <a href="https://www.flickr.com/photos/141699465@N07/sets/72157666870846705" style="font-size:200%">Link to Flickr Album</a>
            <div class="divider"></div>
            <% @photosets[2].each do |image| %>
            <div class="col s6 l4">
                <div class="card hoverable">
                    <div class="card-image">
                        <%= image_tag image, class: "materialboxed" %>
                    </div>
                </div>
            </div>
            <% end %>
        </div>

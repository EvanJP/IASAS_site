<% provide(:title, "Gallery") %>
<div class="container">
    <div class="row">
        <div class="col s12">
            <ul class="tabs">
                <li class="tab col s1"><a href="#sprints">Sprints</a></li>
                <li class="tab col s1"><a href="#midd">Mid Distance</a></li>
                <li class="tab col s1"><a href="#longd">Long Distance</a></li>
                <li class="tab col s1"><a href="#relays">Relays</a></li>
                <li class="tab col s1"><a href="#throws">Throws</a></li>
                <li class="tab col s1"><a href="#hurdles">Hurdles</a></li>
                <li class="tab col s1"><a href="#jumps">Jumps</a></li>
